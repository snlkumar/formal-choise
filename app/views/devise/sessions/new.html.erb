
      <div class="row">
      	<div class="span4">					
						<h4 class="title"><span class="text"><strong>Return</strong> User</span></h4>
						<%= form_for(resource, :as => resource_name, :url => user_session_path,:html=>{:class=>"bs-example form-horizontal"}) do |f| %>
							 <%= render 'devise/shared/messages', model: resource %>
							<input type="hidden" name="next" value="/">
							<fieldset>
								<div class="control-group">
									<%= f.label :email,:class=>"control-label" %>
									<div class="controls">
										<%= f.email_field :email, :autofocus => true %>										
									</div>
								</div>
								<div class="control-group">
									<%= f.label :password,:class=>"control-label" %>
									<div class="controls">
										<%=f.password_field :password%>
									</div>
								</div>
								<div class="control-group">
									<input tabindex="3" class="btn btn-inverse large" type="submit" value="Sign into your account">
									<hr>
									<% if devise_mapping.rememberable? -%>
								   <div class="control-group">
									<%= f.label :remember_me,:class=>"control-label" %>
									
									<%= f.check_box :remember_me %>
								  </div>
								 <%end%>
								</div>
								 
							</fieldset>
						<%end%>		
						<% render "devise/shared/links" %>		
					</div>
