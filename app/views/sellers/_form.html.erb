              	<%= form_for(@seller) do |f| %>
              	<div class="clr"></div>
				  	<% if @seller.errors.any? %>   
				      <ul class="alert fade in alert-error">
				      <% @seller.errors.full_messages.each do |msg| %>
				        <li><%= msg %></li>
				      <% end %>
				      </ul>
				    
				  <% end %>
                <fieldset>
                  <legend>Legend</legend>
                  <div class="form-group">
                    <label for="inputEmail" class="col-lg-2 control-label"><%=f.label :first_name%></label>
                    <div class="col-lg-4">
                      <%= f.text_field :first_name, :required => true %>
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="inputPassword" class="col-lg-2 control-label"><%=f.label :last_name%></label>
                    <div class="col-lg-4">
                         <%= f.text_field :last_name, :required => true %>                      
                    </div>
                  </div>                  
                  <%= f.fields_for :user do |u| %>
                  <div class="form-group">
                    <label for="inputEmail" class="col-lg-2 control-label"><%=u.label :email%></label>
                    <div class="col-lg-4">
                      <%= u.email_field :email, :required => true, :autofocus => true,:class=>"text_class" %>
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="inputPassword" class="col-lg-2 control-label"><%=u.label :password%></label>
                    <div class="col-lg-4">
                         <%= u.password_field :password, :required => true %>                      
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="inputPassword" class="col-lg-2 control-label"><%=u.label :password_confirmation%></label>
                    <div class="col-lg-4">
                         <%= u.password_field :password_confirmation, :required => true %>                      
                    </div>
                  </div>
                  <%end%>
                  <div class="form-group">
                    <label for="inputEmail" class="col-lg-2 control-label"><%=f.label :address%></label>
                    <div class="col-lg-4">
                      <%= f.text_field :address, :required => true %>
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="inputPassword" class="col-lg-2 control-label"><%=f.label :mobile%></label>
                    <div class="col-lg-4">
                         <%= f.text_field :mobile, :required => true %>                      
                    </div>
                  </div>
                  <div class="form-group">
                    <div class="col-lg-4 col-lg-offset-2">
                      <button class="btn btn-default">Cancel</button> 
                      <button type="submit" class="btn btn-primary">Submit</button> 
                    </div>
                  </div>
                </fieldset>
              <%end%>
            
          